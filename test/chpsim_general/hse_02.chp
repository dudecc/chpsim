type e1of2 = (e-; d0-, d1-);

process X()(O! : e1of2)
hse { [O.e]; O.d0+; [~O.e]; O.d0-; [O.e]; O.d1+; [~O.e]; O.d1- }

process Y()(I? : e1of2)
hse { <<; i : 0..1 : I.e+; [I.d0 -> print(0) [] I.d1 -> print(1)]; I.e-; [~I.d0 & ~I.d1] >> }

process Z()(I? : e1of2; O! : e1of2)
meta { /* connect I, O; */ }

process main()()
meta {
  instance x : X;
  instance y : Y;
  instance z : Z;
  connect x.O, z.I;
  connect z.O, y.I;
}

