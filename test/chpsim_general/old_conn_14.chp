
process X()(O(e; d[0..1]-))
chp { [O.e]; O.d[0]+; [~O.e]; O.d[0]-; [O.e]; O.d[1]+; [~O.e]; O.d[1]- }

process Y()(I(d[0..1]; e-))
chp { <<; i : 0..1 : I.e+; [I.d[0] -> print(0) [] I.d[1] -> print(1)]; I.e-; [~I.d[0] & ~I.d[1]] >> }

process XX()(O(e; d[0..1]-))
meta { instance x : X; connect O, x.O; }

process main()()
meta {
  instance x : XX;
  instance y : Y;
  connect x.O, y.I;
}

